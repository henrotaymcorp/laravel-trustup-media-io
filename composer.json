{
    "name": "henrotaym/laravel-trustup-media-io",
    "description": "Used by trustup applications to store and retrieve media",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^2.0",
        "henrotaym/laravel-api-client": "^2.0",
        "deegitalbe/server-authorization": "^2.0",
        "henrotaym/laravel-trustup-media-io-common": "^2.0",
        "deegitalbe/laravel-trustup-io-external-model-relations": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Henrotaym\\LaravelTrustupMediaIo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Henrotaym\\LaravelTrustupMediaIo\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Henrotaym\\LaravelTrustupMediaIo\\Providers\\LaravelTrustupMediaIoServiceProvider"
            ]
        }
    }
}
