{
    "name": "henrotaym/laravel-trustup-media-io",
    "description": "Used by trustup applications to store and retrieve media",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^1.3",
        "henrotaym/laravel-api-client": "^1.5.2",
        "deegitalbe/server-authorization": "^1.2",
        "henrotaym/laravel-trustup-media-io-common": "^1.3",
        "deegitalbe/laravel-trustup-io-external-model-relations": "^1.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5",
        "larastan/larastan": "^1.0",
        "laravel/pint": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Henrotaym\\LaravelTrustupMediaIo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Henrotaym\\LaravelTrustupMediaIo\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Henrotaym\\LaravelTrustupMediaIo\\Providers\\LaravelTrustupMediaIoServiceProvider"
            ]
        }
    }
}
